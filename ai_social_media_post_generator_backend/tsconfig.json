{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "allowJs": true,
    "checkJs": true,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "noImplicitOverride": false,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "noEmit": true,
    "allowImportingTsExtensions": true
  }
}

